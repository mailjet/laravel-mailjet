# Mailjet API credentials
# Get your keys from: https://app.mailjet.com/account/api_keys
MAILJET_APIKEY=your_api_key
MAILJET_APISECRET=your_api_secret

# Mail configuration
MAIL_FROM_ADDRESS=your@email.com
MAIL_FROM_NAME=YourName

# Sandbox mode - when true, emails are validated but not sent (useful for testing)
MAILJET_SANDBOX=false
